<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bydbishe.mapper.LoginMapper">
    <select id="getByUsernameInAdmin" resultType="com.bydbishe.entity.Admin">
        select * from cadmin where username = #{username}
    </select>
    
    <select id="getByUsernameInStudent" resultType="com.bydbishe.entity.Student">
        select * from cstudent where username = #{username}
    </select>
</mapper>